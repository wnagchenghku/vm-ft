VER=3.6
APP_ROOT=`pwd`
rm -rf mongoose out
if [ ! -f mongoose-$VER.tgz ]; then
wget http://mongoose.googlecode.com/files/mongoose-$VER.tgz
fi
tar zxvf mongoose-$VER.tgz
cd mongoose
make linux
mv mongoose ../mg-server
cd $APP_ROOT/mongoose
ln -s $APP_ROOT/../apache/install/bin/ab .
cp $COLO/apps/apache/test.php ~/
cp $COLO/apps/apache/test.php ./
